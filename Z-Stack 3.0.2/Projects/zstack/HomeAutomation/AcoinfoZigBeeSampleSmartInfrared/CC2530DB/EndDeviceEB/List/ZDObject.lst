   2705          /*********************************************************************
   2706           * @fn      ZDMatchSendState()
   2707           *
   2708           * @brief   State machine for the End device match message algorithm.
   2709           *
   2710           * @param  reason - state of algoritm
   2711           * @param  status - initial message status
   2712           * @param  TransSeq - next transaction sequence number
   2713           *
   2714           * @return  FALSE if error and we are not currently matching, TRUE
   2715           *          if success.
   2716           */
   2717          uint8 ZDMatchSendState( uint8 reason, uint8 status, uint8 TransSeq )
   2718          {
   2719            uint8 *dstIEEEAddr = NULL;
   2720            uint8 dstEP = 0xFF;
   2721            zAddrType_t dstAddr;
   2722            zAddrType_t destinationAddr;
   2723            uint16 msgType;
   2724            uint16 clusterID = 0xFFFF;
   2725            ZDEndDeviceBind_t *ed = NULL;
   2726            uint8 rspStatus = ZDP_SUCCESS;
   2727          
   2728            if ( matchED == NULL )
   2729            {
   2730              return ( FALSE );
   2731            }
   2732          
   2733            // Check sequence number
   2734            if ( reason == ZDMATCH_REASON_BIND_RSP || reason == ZDMATCH_REASON_UNBIND_RSP )
   2735            {
   2736              if ( TransSeq != matchED->transSeq )
   2737              {
   2738                return( FALSE ); // ignore the message
   2739              }
   2740            }
   2741          
   2742            // turn off timer
   2743            APS_SetEndDeviceBindTimeout( 0, ZDO_EndDeviceBindMatchTimeoutCB );
   2744          
   2745            if ( reason == ZDMATCH_REASON_TIMEOUT )
   2746            {
   2747              rspStatus = ZDP_TIMEOUT;    // The process will stop
   2748            }
   2749          
   2750            if ( reason == ZDMATCH_REASON_START || reason == ZDMATCH_REASON_BIND_RSP )
   2751            {
   2752              matchED->sending = ZDMATCH_SENDING_UNBIND;
   2753          
   2754              if ( reason == ZDMATCH_REASON_BIND_RSP && status != ZDP_SUCCESS )
   2755              {
   2756                rspStatus = status;
   2757              }
   2758            }
   2759            else if ( reason == ZDMATCH_REASON_UNBIND_RSP )
   2760            {
   2761              if ( status == ZDP_SUCCESS )
   2762              {
   2763                matchED->sending = ZDMATCH_SENDING_UNBIND;
   2764              }
   2765              else
   2766              {
   2767                matchED->sending = ZDMATCH_SENDING_BIND;
   2768              }
   2769            }
   2770          
   2771            if ( reason != ZDMATCH_REASON_START && matchED->sending == ZDMATCH_SENDING_UNBIND )
   2772            {
   2773              // Move to the next cluster ID
   2774              if ( matchED->ed1numMatched )
   2775              {
   2776                matchED->ed1numMatched--;
   2777              }
   2778              else if ( matchED->ed2numMatched )
   2779              {
   2780                matchED->ed2numMatched--;
   2781              }
   2782            }
   2783          
   2784            // What message do we send now
   2785            if ( matchED->ed1numMatched )
   2786            {
   2787              ed = &(matchED->ed1);
   2788              clusterID = matchED->ed1Matched[matchED->ed1numMatched-1];
   2789              dstIEEEAddr = matchED->ed2.ieeeAddr;
   2790              dstEP = matchED->ed2.endpoint;
   2791            }
   2792            else if ( matchED->ed2numMatched )
   2793            {
   2794              ed = &(matchED->ed2);
   2795              clusterID = matchED->ed2Matched[matchED->ed2numMatched-1];
   2796              dstIEEEAddr = matchED->ed1.ieeeAddr;
   2797              dstEP = matchED->ed1.endpoint;
   2798            }
   2799          
   2800            dstAddr.addrMode = Addr16Bit;
   2801          
   2802            // Send the next message
   2803            if ( (rspStatus == ZDP_SUCCESS) && ed )
   2804            {
   2805              // Send unbind/bind message to source
   2806              if ( matchED->sending == ZDMATCH_SENDING_UNBIND )
   2807              {
   2808                msgType = Unbind_req;
   2809              }
   2810              else
   2811              {
   2812                msgType = Bind_req;
   2813              }
   2814          
   2815              dstAddr.addr.shortAddr = ed->srcAddr;
   2816          
   2817              // Save off the transaction sequence number
   2818              matchED->transSeq = ZDP_TransID;
   2819          
   2820              destinationAddr.addrMode = Addr64Bit;
   2821              osal_cpyExtAddr( destinationAddr.addr.extAddr, dstIEEEAddr );
   2822          
   2823              ZDP_BindUnbindReq( msgType, &dstAddr, ed->ieeeAddr, ed->endpoint, clusterID,
   2824                  &destinationAddr, dstEP, ed->SecurityUse );
   2825          
   2826              // Set timeout for response
   2827              APS_SetEndDeviceBindTimeout( AIB_MaxBindingTime, ZDO_EndDeviceBindMatchTimeoutCB );
   2828            }
   2829            else
   2830            {
   2831              // Send the response messages to requesting devices
   2832              // send response to first requester
   2833              dstAddr.addr.shortAddr = matchED->ed1.srcAddr;
   2834              ZDP_EndDeviceBindRsp( matchED->ed1.TransSeq, &dstAddr, rspStatus, matchED->ed1.SecurityUse );
   2835          
   2836              // send response to second requester
   2837              if ( matchED->state == ZDMATCH_SENDING_BINDS )
   2838              {
   2839                dstAddr.addr.shortAddr = matchED->ed2.srcAddr;
   2840                ZDP_EndDeviceBindRsp( matchED->ed2.TransSeq, &dstAddr, rspStatus, matchED->ed2.SecurityUse );
   2841              }
   2842          
   2843              // Process ended - release memory used
   2844              ZDO_RemoveMatchMemory();
   2845            }
   2846          
   2847            return ( TRUE );
   2848          }
